	<!--  Capabilities -->
	<element name="capability.operatingSystem" substitutionGroup="core:capability" type="os:OperatingSystem" />
	<complexType name="OperatingSystem">
		<complexContent>
			<extension base="core:Capability">
				<attribute name="hostname" type="string">
					<annotation>
						<documentation>
							A hostname, which could be a simple hostname or qualified name up to a full fqdn. In many
							operating systems, what would be returned by "hostname" from a command line.
						</documentation>
					</annotation>
				</attribute>
				<attribute name="os.type" type="string">
					<annotation>
						<documentation>
							e.g. "AIX", "Windows-Cygwin", "HPUX", "Linux", "Solaris", "Windows". Use the values from the
							OperatingSystemType enumeration if applicable.
						</documentation>
					</annotation>
				</attribute>

				<attribute name="os.distribution" type="string">
					<annotation>
						<documentation>e.g. "SuSE" if osType is "Linux"</documentation>
					</annotation>
				</attribute>

				<attribute name="os.version" type="string">
					<annotation>
						<documentation>e.g. "4.0"</documentation>
					</annotation>
				</attribute>

				<attribute name="os.servicepack" type="string">
					<annotation>
						<documentation>e.g. "SP5" for Windows 2000</documentation>
					</annotation>
				</attribute>
				<attribute name="fqdn" type="string">
					<annotation>
						<documentation>
							Fully qualified domain name. e.g. "foo.watson.ibm.com." In practice the trailing "." is
							often omitted.
						</documentation>
					</annotation>
				</attribute>
				<attribute name="kernelArchitecture" type="string">
					<annotation>
						<documentation>e.g. Intel (Windows), or i686 (Linux). TODO an enumeration?</documentation>
					</annotation>
				</attribute>
				<attribute name="kernelVersion" type="string">
					<annotation>
						<documentation>e.g. 5.1.2600 (Windows), or 2.6.9-55.0.2.ELsmp (Linux).</documentation>
					</annotation>
				</attribute>
				<attribute name="kernelWidth" type="os:KernelWidthType">
					<annotation>
						<documentation>An enumeration, "32-bit" or "64-bit"</documentation>
					</annotation>
				</attribute>
				<attribute name="manufacturer" type="string">
					<annotation>
						<documentation>TODO</documentation>
					</annotation>
				</attribute>
				<attribute name="locale" type="string">
					<annotation>
						<documentation>In Linux, e.g. “en_US”. In Windows WMI, e.g. “0409”</documentation>
					</annotation>
				</attribute>
				<attribute name="TimeZone" type="string" />
			</extension>
		</complexContent>
	</complexType>

