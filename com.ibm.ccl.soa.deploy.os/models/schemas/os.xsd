	<element name="capability.aixLocalUser" substitutionGroup="core:capability" type="os:AIXLocalUser" />
	<complexType name="AIXLocalUser">
		<complexContent>
			<extension base="os:User">
				<attribute name="UserName" type="string" />
				<attribute name="FullName" type="string" />
				<!-- "AIX Local User"  -->
				<attribute name="AccountDescription" type="string" />
				<!-- User must change password at next logon - default false -->
				<attribute name="ChangePasswordNextLogon" type="boolean" />
				<!-- User cannot change password - default false -->
				<attribute name="UserCannotChangePassword" type="boolean" />
				<!-- Password never expires - default true -->
				<attribute name="PasswordNeverExpires" type="boolean" />
				<!-- User cannot log in - default false -->
				<attribute name="AccountDisabled" type="boolean" />
				<!-- User shell -->
				<attribute name="Shell" type="string" />
				<!-- User home directory -->
				<attribute name="HomeDir" type="string" />

				<attribute name="UserSid" type="string" />
				<!-- The administrative status of the user -->
				<attribute name="IsAdmin" type="boolean" />
				<!--  This should be a collection of strings... -->
				<!-- <attribute name="ManagedGroups" type="string" /> -->
				<attribute name="PrimaryAuthMethod" type="os:AIXAuthMethodType" />
				<attribute name="SecondaryAuthMethod" type="os:AIXAuthMethodType" />
				<attribute name="AccountExpirationTime" type="string" />
				<attribute name="PassReuseNotAllowedInWeeks" type="integer" />
				<attribute name="NotReusableNumberOfPasswords" type="integer" />
				<attribute name="LoginPermitted" type="boolean" />
				<attribute name="LoginTimes" type="integer" />
				<attribute name="LoginRetries" type="integer" />
				<!-- The maximum number of weeks a password is valid (0 is equivalent to unlimited) - default 0 -->
				<attribute name="MaxPasswordAgeInWeeks" type="integer" />
				<!--   maximum number of weeks after MaxPasswordAgeInWeeks that an expired password can be changed by the user (-1 is equivalent to unlimited) -->
				<attribute name="MaxPasswordValidWeeks" type="integer" />
				<!-- Maximum number of times a given character can appear in a password (8 is equivalent to unlimited) -->
				<attribute name="MaxCharRepeats" type="integer" />
				<!--  Minimum number of weeks between password changes - Default 0 -->
				<attribute name="MinPasswordAgeInWeeks" type="integer" />
				<!-- The minimum number of alphabetic characters in a password - default 0 -->
				<attribute name="MinAlphaCharsInPassword" type="integer" />
				<!-- The minimum number of characters in the new password that were not in the old password - default 0 -->
				<attribute name="MinDiffChars" type="integer" />
				<!-- The minimum length of the password - default 0  -->
				<attribute name="MinPasswordLength" type="integer" />
				<!--  The minumum number of non-alphabetic character in a password - default 0-->
				<attribute name="MinOtherCharsInPassword" type="integer" />
				<!--  The number of days before a forced password change that a warning will be given to the user (0 is equivalent to disable this feature) -->
				<attribute name="PasswordChangeWarnTime" type="integer" />
				<!-- Where this user is administered -->
				<attribute name="UserRegisteringMethod" type="os:AIXUserRegisteringMethodType" />
				<!-- Whether the user account can be accessed by remote logins -->
				<attribute name="RemoteLoginIsAllowed" type="boolean" />
				<!-- Whether other users can switch to this user account - default true-->
				<attribute name="SuIsAllowed" type="boolean" />
				<!-- Which groups can switch to this user account - default ALL -->
				<attribute name="SuGroupsAllowed" type="string" />
				<!--  Last login time -->
				<attribute name="TimeLastLogin" type="string" />
				<!-- Number of unsuccessful login attempts -->
				<attribute name="UsuccessfulLoginCount" type="integer" />
				<!-- Group membership done with membership links -->

			</extension>
		</complexContent>
	</complexType>


