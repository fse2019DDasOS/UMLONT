	<!-- WASV5DB2 DataSource, add DB2-specific properties 
		See: "Properties for the IBM DB2 Driver for JDBC and SQLJ"
		http://publib.boulder.ibm.com/infocenter/db2luw/v9/index.jsp?topic=/com.ibm.db2.udb.apdv.java.doc/doc/rjvdsprp.htm
	-->
	<element name="capability.wasv5DB2Datasource" substitutionGroup="base:capability" type="was:WasV5DB2Datasource"/>
	<complexType name="WasV5DB2Datasource">
		<complexContent>
			<extension base="was:WasV5Datasource">
				<!-- Just adding a few here, many more may be possible -->
				<attribute name="traceLevel" type="int">
					<annotation>
						<documentation>
						    bit flags, for example com.ibm.db2.jcc.DB2BaseDataSource.TRACE_ALL.
						</documentation>
					</annotation>
				</attribute>
				<attribute name="traceFile" type="string">
					<annotation>
						<documentation>
						    the name of a file into which the IBM DB2 Driver for JDBC and SQLJ writes trace information.
						</documentation>
					</annotation>
				</attribute>
				<attribute name="fullyMaterializeLobData" type="boolean">
					<annotation>
						<documentation>
						    shall the driver retrieve LOB locators for FETCH operations?
						</documentation>
					</annotation>
				</attribute>
				<attribute name="resultSetHoldability" type="int">
					<annotation>
						<documentation>
						    Specifies whether cursors remain open after a commit operation.
						</documentation>
					</annotation>
				</attribute>
				<attribute name="currentPackageSet" type="string">
					<annotation>
						<documentation>
						    The collection ID to search for JDBC and SQLJ packages.
						</documentation>
					</annotation>
				</attribute>
				<attribute name="deferPrepares" type="boolean">
					<annotation>
						<documentation>
						    Specifies whether to defer prepare operations until statement execution.
						</documentation>
					</annotation>
				</attribute>
				<attribute name="currentSchema" type="string">
					<annotation>
						<documentation>
						    The default schema name that is used to qualify unqualified database objects in dynamically prepared SQL statements.
						</documentation>
					</annotation>
				</attribute>
				<attribute name="cliSchema" type="string">
					<annotation>
						<documentation>
						    AKA sysSchema, the schema of the DB2 shadow catalog tables or views that are searched when an application invokes a DatabaseMetaData method.
						</documentation>
					</annotation>
				</attribute>
				
				<!--  TODO All the above attributes are properties of the IBM DB2 driver.  'useTemplate'
					is different.  I suspect it means that the datasource (was created/should be created)
					using AdminConfig.createUsingTemplate() instead of AdminConfig.create().  If that's
					true then we have a problem, because we don't model a 'templateName' or ID.
					Using a template seems to change the meaning of other fields, for example if a 
					template is used the 'dataSourceHelperClassName' field can be blank, for we will
					inherit it, but it probably should be a validation error if the field is blank and we
					aren't using a template! -->
				<attribute name="useTemplate" type="boolean"/>				
			</extension>
		</complexContent>
	</complexType>
