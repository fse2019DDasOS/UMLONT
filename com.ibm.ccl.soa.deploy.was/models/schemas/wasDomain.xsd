    <!-- This corresponds to WebSphere's DataSource MBean, see
         http://publib.boulder.ibm.com/infocenter/eserver/v5r3/ic2924/info/rzatz/51/admin/apidocs/mbean/DataSource.html
         http://publib.boulder.ibm.com/infocenter/wasinfo/v5r1//index.jsp?topic=/com.ibm.websphere.zseries.doc/info/zseries/javadoc/wccm/resources.jdbc/DataSource.html
      -->
	<element name="capability.wasDatasource" substitutionGroup="base:capability" type="was:WasDatasource"/>
	<complexType name="WasDatasource">
		<annotation>
			<documentation>
			    Representation of WebSphere's DataSource MBean
			</documentation>
		</annotation>
		<complexContent>
			<extension base="j2ee:J2EEDatasource">
					<attribute name="category" type="string">
						<annotation>
							<documentation>
							    Optional J2EEResourceFactory category, used as a grouping mechanism.
							</documentation>
						</annotation>
					</attribute>
					<attribute name="statementCacheSize" type="string">
						<annotation>
							<documentation>
							    Number of free prepared statements per connection.
							</documentation>
						</annotation>
					</attribute>
					<attribute name="minConnections" type="string">	
						<annotation>
							<documentation>
							    Minimum number of managed connection that should be maintained for all cases except if agedTimeout is set.
							</documentation>
						</annotation>
					</attribute>
					<attribute name="maxConnections" type="string">
						<annotation>
							<documentation>
								Maximum number of managed connections that can be created in this connection pool.
							</documentation>
						</annotation>
					</attribute>
					<attribute name="connectionTimeout" type="string">	
						<annotation>
							<documentation>
								Interval in seconds after which a connection request times out.
							</documentation>
						</annotation>
					</attribute>
			</extension>
		</complexContent>
	</complexType>
	
	<!--
		This corresponds to WebSphere's DataSource MBean, see
        http://publib.boulder.ibm.com/infocenter/eserver/v5r3/ic2924/info/rzatz/51/admin/apidocs/mbean/DataSource.html
        http://publib.boulder.ibm.com/infocenter/wasinfo/v6r0/index.jsp?topic=/com.ibm.websphere.express.doc/info/exp/ae/rdat_chgconwsadmin.html
		http://publib.boulder.ibm.com/infocenter/wasinfo/v5r1//index.jsp?topic=/com.ibm.websphere.zseries.doc/info/zseries/javadoc/wccm/resources.jdbc/DataSource.html 
	 -->
	<element name="capability.wasv5datasource" substitutionGroup="base:capability" type="was:WasV5Datasource"/>
	<complexType name="WasV5Datasource">
		<complexContent>
			<extension base="was:WasDatasource">
			   <!--  TODO Limiting the class name to an enum seems wrong... -->
					<attribute name="dataSourceHelperClassName" type="was:DataSourceHelperClassNames"/>
					<attribute name="reapTime" type="string">
						<annotation>
							<documentation>
							    Interval in seconds between runs of the connection pool maintenance thread.
							</documentation>
						</annotation>
					</attribute>
					<attribute name="unusedTimeout" type="string">
						<annotation>
							<documentation>
							    Interval in seconds after which an unused connection is discarded by the connection pool maintenance thread.
							</documentation>
						</annotation>
					</attribute>
					<attribute name="agedTimeout" type="string">
						<annotation>
							<documentation>
							    Interval in seconds after which an unused aged connection is discarded by the connection pool maintenance thread.
							</documentation>
						</annotation>
					</attribute>
					<attribute name="purgePolicy" type="was:Policies">	
						<annotation>
							<documentation>
							    How to purge connections when a stale connection or fatal connection error is detected.
							</documentation>
						</annotation>
					</attribute>
					<!--  TODO What is this? -->	
					<attribute name="j2cAuthAlias" type="string"/>		
			</extension>
		</complexContent>
	</complexType>
	
