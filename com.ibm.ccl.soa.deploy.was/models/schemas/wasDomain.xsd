    <complexType name="WasApplicationExt">
    	<complexContent>
    		<extension base="base:Capability">
    			<attribute name="reloadInterval" type="long"
    				use="optional">
    			</attribute>
    			<attribute name="shareSessionContext" type="boolean" use="optional"></attribute>
    		</extension>
    	</complexContent>
    </complexType>

    <element name="capability.wasApplicationExt" type="was:WasApplicationExt"></element>

    <complexType name="WasWebAppExt">
    	<complexContent>
    		<extension base="base:Capability">
    			<attribute name="reloadInterval" type="int"
    				use="optional">
    			</attribute>
    			<attribute name="reloadingEnabled" type="boolean" use="optional"></attribute>
    			<attribute name="defaultErrorPage" type="string" use="optional"></attribute>
    			<attribute name="additionalClassPath" type="string" use="optional"></attribute>
    			<attribute name="fileServingEnabled" type="boolean" use="optional"></attribute>
    			<attribute name="directoryBrowsingEnabled"
    				type="boolean" use="optional">
    			</attribute>
    			<attribute name="serveServletsByClassnameEnabled"
    				type="boolean" use="optional">
    			</attribute>
    			<attribute name="preCompileJSPs" type="boolean" use="optional"></attribute>
    			<attribute name="autoRequestEncoding" type="boolean" use="optional"></attribute>
    			<attribute name="autoResponseEncoding" type="boolean" use="optional"></attribute>
    			<attribute name="autoLoadFilters" type="boolean" use="optional"></attribute>
    		</extension>
    	</complexContent>
    </complexType>

    <element name="capability.wasWebAppExt" type="was:WasWebAppExt"></element>
    
    <element name="constraint.wasSecuritySubjectConstraint" substitutionGroup="base:constraint" type="was:WasSecuritySubjectConstraint"></element>
    <complexType name="WasSecuritySubjectConstraint">
    	<annotation>
    		<documentation xml:lang="en">
    			A security role mapping from a J2EE security role to a
    			Was security subject.
    		</documentation>
    	</annotation>
    	<complexContent>
    		<extension base="was:WasJ2EEConstraint">

    			<attribute name="wasSecuritySubject" type="string"
    				use="required" />
    		</extension>
    	</complexContent>
    </complexType>

    <element name="constraint.wasJ2EEConstraint" substitutionGroup="base:constraint" type="was:WasJ2EEConstraint"></element>
    <complexType name="WasJ2EEConstraint">
        <annotation>
        	<documentation>
        		Super type for all J2EE specific was domain constraints.
        	</documentation>
        </annotation>
        <complexContent>
    		<extension base="base:Constraint"></extension>
    	</complexContent>
    </complexType>

	<element name="constraint.wasModuleClassloaderPolicyConstraint" substitutionGroup="base:constraint" type="was:WasModuleClassloaderPolicyConstraint"></element>
    <complexType name="WasModuleClassloaderPolicyConstraint">
        <annotation>
        	<documentation>
        		Super type for all classloader policy was constraints.
        	</documentation>
        </annotation>
        <complexContent>
    		<extension base="was:WasJ2EEConstraint"></extension>
    	</complexContent>
    </complexType>

	<element name="constraint.wasWarClassloaderPolicyConstraint" substitutionGroup="base:constraint" type="was:WasWarClassloaderPolicyConstraint"></element>
    <complexType name="WasWarClassloaderPolicyConstraint">
        <annotation>
        	<documentation>
        		Was classloader policy constraint for war modules.
        	</documentation>
        </annotation>
        <complexContent>
    		<extension
    			base="was:WasModuleClassloaderPolicyConstraint">
    			<attribute name="order" type="was:ClassloaderMode"></attribute>
    		</extension>
    	</complexContent>
    </complexType>

	<element name="constraint.wasEarClassloaderPolicyConstraint" substitutionGroup="base:constraint" type="was:WasEarClassloaderPolicyConstraint"></element>
    <complexType name="WasEarClassloaderPolicyConstraint">
        <annotation>
        	<documentation>
        		Was classloader policy constraint for ear modules.
        	</documentation>
        </annotation>
        <complexContent>
    		<extension base="was:WasWarClassloaderPolicyConstraint">
    			<attribute name="reloadInterval" type="int"></attribute>
    			<attribute name="reloadClasses" type="boolean"></attribute>
    			<attribute name="policy" type="was:ClassloaderPolicyType"></attribute>
    		</extension>
    	</complexContent>
    </complexType>

	<element name="constraint.wasModuleStartWeightConstraint" type="was:WasModuleStartWeightConstraint" substitutionGroup="base:constraint"></element>
    <complexType name="WasModuleStartWeightConstraint">
        <annotation>
        	<documentation>
        		Was module starting weight constraint.
        	</documentation>
        </annotation>
        <complexContent>
    		<extension base="was:WasJ2EEConstraint">
    			<attribute name="startingWeight" type="int"></attribute>
    		</extension>
    	</complexContent>
    </complexType>

	<element name="constraint.wasJNDIBindingConstraint" type="was:WasJNDIBindingConstraint" substitutionGroup="base:constraint">
		<annotation>
			<documentation>
				Was JNDI binding constraint, used for binding J2EE
				capabilities to specific JNDI names on a was server,
				cluster, cell, or node.
			</documentation>
		</annotation></element>
    <complexType name="WasJNDIBindingConstraint">
    	<complexContent>
    		<extension base="was:WasJ2EEConstraint">
    			<attribute name="jndiName" type="string"></attribute>
    		</extension>
    	</complexContent>
    </complexType>

</schema>