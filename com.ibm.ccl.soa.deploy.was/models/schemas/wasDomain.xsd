    <simpleType name="MessageReliabilityEnum">
    	<restriction base="string"> 
    	
    	<enumeration value="None">
        <annotation>
          <documentation>
                There is no message reliability for nonpersistent messages. If a nonpersistent message cannot be delivered, it is discarded.
			</documentation>
        </annotation>
      </enumeration>    	
        <enumeration value="Best effort nonpersistent">
        <annotation>
          <documentation>
                Messages are never written to disk, and are thrown away if memory cache overruns. 
			</documentation>
        </annotation>
      </enumeration>       
       <enumeration value="Express nonpersistent">
        <annotation>
          <documentation>
                Messages are written asynchronously to persistent storage if memory cache overruns, but are not kept over server restarts. 
			</documentation>
        </annotation>
      </enumeration>    
       <enumeration value="Reliable nonpersistent">
        <annotation>
          <documentation>
                Messages can be lost if a messaging engine fails, and can be lost under normal operating conditions. 
			</documentation>
        </annotation>
      </enumeration>
       <enumeration value="Reliable persistent">
        <annotation>
          <documentation>
                Messages can be lost if a messaging engine fails, but are not lost under normal operating conditions.
			</documentation>
        </annotation>
      </enumeration>
       <enumeration value="Assured persistent">
        <annotation>
          <documentation>
                Highest degree of reliability where assured message delivery is supported.
			</documentation>
        </annotation>
      </enumeration>       
       <enumeration value="As Bus destination">
        <annotation>
          <documentation>
                Use the delivery option configured for the bus destination. 
			</documentation>
        </annotation>
       </enumeration>     		    		
    </restriction>
    </simpleType>
    
    <simpleType name="ReadAheadOptimizationEnum">
    	<restriction base="string">    	  
    		<enumeration value="Default"/>
    		<enumeration value="AlwaysOn"/>
    		<enumeration value="AlwaysOff"/>	    		
    	</restriction>
    </simpleType> 
            
    <simpleType name="ShareDurableSubscriptionsEnum">
    	<restriction base="string">   
    		<enumeration value="In cluster">
        <annotation>
          <documentation>
                Allows sharing of durable subscriptions when connections are made from within a server cluster.
			</documentation>
        </annotation>
      </enumeration>
    		<enumeration value="Always shared">
        <annotation>
          <documentation>
                Durable subscriptions can be shared across connections.
			</documentation>
        </annotation>
      </enumeration>
    		<enumeration value="Never shared">
        <annotation>
          <documentation>
                Durable subscriptions are never shared across connections.
			</documentation>
        </annotation>
      </enumeration>	    		
    	</restriction>
    </simpleType>
    
    <simpleType name="ConnectionDeliveryModeEnum">
