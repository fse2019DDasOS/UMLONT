<?xml version="1.0" encoding="ISO-8859-1"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"
        targetNamespace="urn:www-collation-com:1.0"
        xmlns="urn:www-collation-com:1.0"
        xmlns:coll="urn:www-collation-com:1.0"
        elementFormDefault="qualified" version="1.0">
  <xsd:include schemaLocation="../../../../../com/collation/platform/model/Configuration.xsd"/>
  <xsd:complexType name="com.collation.platform.model.plugin.PluginDefinition">
    <xsd:complexContent>
      <xsd:extension base="coll:com.collation.platform.model.Configuration">
        <xsd:sequence>
          <xsd:element name="pluginType" type="xsd:string" nillable="true" minOccurs="0"/>
          <xsd:element name="commandName" type="xsd:string" nillable="true" minOccurs="0"/>
          <xsd:element name="localWorkingDirectory" type="xsd:string" nillable="true" minOccurs="0"/>
          <xsd:element name="commandRemote" type="xsd:boolean" nillable="true" minOccurs="0"/>
          <xsd:element name="remoteWorkingDirectory" type="xsd:string" nillable="true" minOccurs="0"/>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
</xsd:schema>
